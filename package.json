  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack/index.js",
    "dev": "node --trace-deprecation ./node_modules/.bin/webpack-dev-server --config webpack/index.js",
    "dev:debug": "NODE_ENV=profile npm run dev",
    "test:coverage": "NODE_ENV=test nyc --nycrc-path './.nycrc.yml' npm run test",
    "test": "TZ=UTC NODE_ENV=test NODE_OPTIONS='--max-old-space-size=6144' jest --no-compilation-cache --clearMocks --forceExit",
    "test:profile": "NODE_ENV=test NODE_OPTIONS='--max-old-space-size=6144' node --no-compilation-cache --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage --detectOpenHandles",
    "test:debug": "npm run test:profile -- --inspect-brk",
    "jscpd": "npm run jscpd:run 'app/javascript/**'",
    "jscpd:run": "jscpd",
    "lint": "npm run lint:run -- --fix",
    "lint:run": "eslint './app/javascript/**'"
  },
  "overrides": {
    "minimist": "1.2.7",
    "ansi-regex": "5.0.1",
